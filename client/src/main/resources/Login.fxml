<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="generalLayout" prefHeight="900.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="group10.client.controller.LoginController">
     <TextField alignment="CENTER" layoutX="271.0" layoutY="397.0" prefHeight="53.0" prefWidth="259.0" promptText="password">
      <font>
         <Font size="18.0" />
      </font></TextField>
     <TextField alignment="CENTER" layoutX="271.0" layoutY="326.0" prefHeight="53.0" prefWidth="259.0" promptText="username">
      <font>
         <Font size="18.0" />
      </font></TextField>
     <Button fx:id="loginButton" alignment="CENTER" layoutX="275.0" layoutY="483.0" onAction="#loginClick" prefHeight="41.0" prefWidth="106.0" text="Login">
      <font>
         <Font name="System Bold" size="18.0" />
      </font></Button>
     <Button fx:id="signUpButton" alignment="CENTER" layoutX="419.0" layoutY="483.0" onAction="#signUpClick" prefHeight="41.0" prefWidth="106.0" text="Sign Up">
      <font>
         <Font name="System Bold" size="18.0" />
      </font></Button>
     <Button fx:id="playButton" layoutX="381.0" layoutY="654.0" onAction="#playClick">Play</Button>
</AnchorPane>
